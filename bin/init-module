#!/bin/bash

execdir="$(pushd `dirname $0` >/dev/null ; pwd ; popd >/dev/null)"
basedir="$(pushd ${execdir}/.. >/dev/null ; pwd ; popd >/dev/null)"

module=$1
if [ -n "${module}" ]
then
  ${basedir}/modules/${module}/.init-module
else
  echo "No module given"
  echo "usage: $0 <module>"
  exit 1
fi

exit 0
